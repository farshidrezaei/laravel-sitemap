{
    "name": "farshidrezaei/laravel-sitemap",
    "description": "Sitemap package for Laravel. forked from laravelium sitemap package",
    "homepage": "https://farshidrezaei.ir",
    "keywords": ["farshidrezaei", "laravel", "php", "sitemap", "generator", "xml", "html", "google-news"],
    "license": "MIT",
    "authors": [
        {
            "name": "Farshid Rezaei",
            "email": "me@farshidrezaei.ir",
            "role": "Developer",
            "homepage": "https://farshidrezaei.ir"
        }
    ],
    "support": {
        "issues": "https://github.com/farshidrezaei/laravel-sitemap/issues",
        "source": "https://github.com/farshidrezaei/laravel-sitemap",
        "wiki": "https://github.com/farshidrezaei/laravel-sitemap/wiki"
    },
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "orchestra/testbench-core": "^6.0",
        "php-coveralls/php-coveralls" : "^2.2"
    },
    "autoload": {
        "psr-0": {
            "Laravelium\\Sitemap": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelium\\Sitemap\\SitemapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
